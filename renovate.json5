{
  "$schema": "https://docs.renovatebot.com/renovate-schema.json",
  "enabled": true,
  "extends": [
    "local>adobe-rnd/spacecat-task-processor"
  ],
  "packageRules": [
    {
      "matchPackageNames": [
        "semantic-release"
      ],
      "allowedVersions": ">=23.0"
    },
    {
      "matchPackageNames": [
        "@adobe/spacecat-shared-data-access"
      ],
      "matchUpdateTypes": [
        "major"
      ],
      "enabled": false
    }
  ]
}
